{
    // Lambdas can be recursive!
    let fib: (i64 -> i64);
    fib = lambda (n: i64) -> i64 {
        if n == 0 {
            return 1;
        } else if n == 1 {
            return 1;
        } else {
            return fib(n - 1) + fib(n - 2);
        };
    };
    for i in [0, 7] {
        print(fib(i));
    };
}
